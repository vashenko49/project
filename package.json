  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "typeorm": "ts-node -P ./tsconfig.json  ./node_modules/typeorm/cli.js -d ./src/config/ormconfig.ts",
    "migration:run": "dotenv -e ./.env -- pnpm run typeorm migration:run",
    "migration:generate": "dotenv -e ./.env -- pnpm run typeorm migration:generate ./src/migrations/<ChangeNameOfMigration>",
    "migration:revert": "dotenv -e ./.env -- pnpm run typeorm migration:revert"
  },
